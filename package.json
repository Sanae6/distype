{
  "name": "boogcord",
  "version": "0.0.2",
  "description": "A light yet robust Discord library written in TypeScript, catered towards developers looking for a final, no compromises solution for creating Discord bots.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "build:install": "npm i --also=dev && npm run build",
    "docs": "rm -rf ./docs && typedoc",
    "docs:install": "npm i --also=dev && npm run docs",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prepublishOnly": "npm run build:install",
    "test:build": "npm run build",
    "test:docs": "npm run docs",
    "test:lint": "npm run lint"
  },
  "keywords": [
    "api",
    "boogcord",
    "bot",
    "client",
    "discord",
    "discord-api",
    "discord-bot",
    "discordapp"
  ],
  "homepage": "https://boogcord.br88c.dev",
  "bugs": {
    "url": "https://github.com/boogcord/boogcord/issues"
  },
  "license": "Apache-2.0",
  "author": "BR88C",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boogcord/boogcord.git"
  },
  "dependencies": {
    "@discordjs/collection": "^0.4.0",
    "@jpbberry/typed-emitter": "^1.2.1",
    "axios": "^0.24.0",
    "discord-api-types": "^0.26.0",
    "form-data": "^4.0.0",
    "ws": "^8.4.0"
  },
  "devDependencies": {
    "@br88c/eslint-config": "^1.2.0",
    "@types/node": "^16.11.17",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=16.13.0",
    "npm": ">=8.1.0"
  },
  "private": false
}
