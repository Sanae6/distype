{
  "name": "distype",
  "version": "0.5.1",
  "description": "A light yet robust Discord library written in TypeScript, catered towards developers looking for a final, no compromises solution for creating Discord bots.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "build:install": "npm i --also=dev && npm run build",
    "docs": "rm -rf ./docs && typedoc",
    "docs:install": "npm i --also=dev && npm run docs",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prepublishOnly": "npm run build:install",
    "test:build": "npm run build",
    "test:docs": "npm run docs",
    "test:lint": "npm run lint"
  },
  "keywords": [
    "api",
    "bot",
    "client",
    "discord",
    "discord-api",
    "discord-bot",
    "discordapp",
    "distype"
  ],
  "homepage": "https://distype.br88c.dev",
  "bugs": {
    "url": "https://github.com/distype/distype/issues"
  },
  "license": "Apache-2.0",
  "author": "BR88C",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distype/distype.git"
  },
  "dependencies": {
    "@discordjs/collection": "^0.5.0",
    "@types/ws": "^8.2.2",
    "discord-api-types": "^0.26.1",
    "form-data": "^4.0.0",
    "undici": "^4.13.0",
    "ws": "^8.4.2"
  },
  "devDependencies": {
    "@br88c/eslint-config": "^1.3.0",
    "@types/node": "^16.11.21",
    "eslint": "^8.8.0",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=16.13.0",
    "npm": ">=8.1.0"
  },
  "private": false
}
