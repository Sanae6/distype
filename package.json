{
  "name": "boogcord",
  "version": "0.0.1",
  "description": "A Discord library written in TypeScript for Node.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm i && rm -rf ./dist && tsc",
    "docs": "rm -rf ./docs && typedoc",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "api",
    "boogcord",
