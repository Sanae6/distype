  "homepage": "https://github.com/boogcord/boogcord#readme",
  "bugs": {
    "url": "https://github.com/boogcord/boogcord/issues"
  },
  "license": "Apache-2.0",
  "author": "BR88C",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boogcord/boogcord.git"
  },
  "dependencies": {
    "@discordjs/collection": "^0.3.2",
    "@jpbberry/typed-emitter": "^1.2.1",
    "axios": "^0.24.0",
    "discord-api-types": "^0.25.2",
    "form-data": "^4.0.0",
    "ws": "^8.3.0"
  },
  "devDependencies": {
    "@br88c/eslint-config": "^1.2.0",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.3"
  },
  "engines": {
    "node": ">=16.13.0",
