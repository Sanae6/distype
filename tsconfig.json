{
  "include": ["./src/**/*"],
  "exclude": [],
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": ["ES2021"],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist",
    "removeComments": false,
    "resolveJsonModule": true,
    "skipLibCheck": false,
    "strict": false,
    "strictNullChecks": true,
    "target": "ES2021"
  }
}
